# Test basic safe_head function
def safe_head(list: List(T, n)) -> T when n > 0 =
  match list do
    [x|_] -> x
  end

# Test safe_length function  
def safe_length(list: List(T, n)) = n

# Test with different numeric constraints
def nonsafe_tail(list: List(T, n)) -> List(T, m) when n > 0 =
  match list do
    [_|xs] -> xs
  end

# Test with different numeric constraints
def safe_tail(list: List(T, n)) -> List(T, m) when m == n - 1 =
  match list do
    [_|xs] -> xs
  end
