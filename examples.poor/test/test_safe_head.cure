# Test program for safe_head function with dependent types
def safe_head(list: List(T, n)) -> T when n > 0 =
  match list do
    [x|_] -> x
  end

def safe_length(list: List(T, n)) = 
  let _ = list in n

def main() =
  let numbers = [1, 2, 3]
  let first = safe_head(numbers)
  let length = safe_length(numbers)
  {first, length}