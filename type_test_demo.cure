# Type Constraint Violation Demo
# This should fail at compile-time with type checking now enabled

module TypeTest do
  export [main/0]
  
  def add(a: Int, b: Int): Int =
    a + b
    
  def main(): Int =
    let x = 42
    let y = "10"  
    add(x, y)    # Type error: passing string to function expecting Int
end