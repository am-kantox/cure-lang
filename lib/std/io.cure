module Std.Io do
  export [
    print/1, println/1, 
    debug/1, error/1
  ]

  # Basic output operations - Erlang FFI
  def_erl print(s: String): Int =
    io:format("~ts", [S]), ok.

  def_erl println(s: String): Int =
    io:format("~ts~n", [S]), ok.

  # Debug and error output
  def debug(s: String): Int =
    # Print debug message to stderr
    0  # Simplified return

  def error(s: String): Int =
    # Print error message to stderr
    0  # Simplified return
end
