module ListTupleDemo do
  export [main/0, test_lists/0, test_tuples/0]

  def test_lists(): Int =
    let numbers = [1, 2, 3]
    let empty_list = []
    let single_item = [42]
    42

  def test_tuples(): Int =
    let pair = {1, 2}
    let triple = {1, 2, 3}
    let empty_tuple = {}
    let mixed = {42, "hello", 3.14}
    42
    
  def main(): Int =
    let list_result = test_lists()
    let tuple_result = test_tuples()
    list_result + tuple_result

end