module TestIsEmpty do
  export [test_empty_is_empty/0, test_nonempty_is_empty/0]
  
  import Std.Vector [is_empty/1]
  
  def test_empty_is_empty(): Bool =
    let empty_vec = []
    is_empty(empty_vec)
    
  def test_nonempty_is_empty(): Bool =
    let nonempty_vec = [1, 2, 3]
    is_empty(nonempty_vec)
    
end