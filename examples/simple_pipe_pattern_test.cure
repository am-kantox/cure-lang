# Simple test that uses pattern matching on pipe result
module SimplePipePattern do
  export [main/0, test_pipe_pattern/0, double/1]

  def main(): Int =
    test_pipe_pattern()

  def test_pipe_pattern(): Int =
    let result = 42 |> double in
    match result do
      Ok(value) -> value
    end

  def double(x: Int): Int =
    x * 2
end
