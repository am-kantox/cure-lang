module TestMainMatch do
  export [main/0]

  def main(): Int =
    let result = Ok(42)
    let _ = match result do
      Ok(value) -> 
        let msg = "Result: {'Ok', " ++ int_to_string(value) ++ "}"
        println(msg)
      Error(reason) -> 
        let msg = "Result: {'Error', \"" ++ reason ++ "\"}"
        println(msg)
      _ ->
        println("Result: Unknown format")
    end
    42
end