# Simple Pattern Test
module SimplePattern do
  export [main/0, double/1]

  def main(): Int =
    let result = 5 |> double
    let _ = match result do
      Ok(value) -> println("Pattern match works!")
      Error(reason) -> println("Got error")
    end
    42
    
  def double(x: Int): Int =
    x * 2
end