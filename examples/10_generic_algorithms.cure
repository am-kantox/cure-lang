module generic_algorithms do
export [main/0]

# Generic Sorting Algorithm using Ord typeclass
def sort(list: List(T)): List(T) where Ord(T) =
    match list do
        [] -> []
        [pivot | rest] -> sort(rest)
    end

# Example Data Types
record Person do
    name: String
    age: Int
end
derive Show, Eq, Ord

def main(): Int =
    0

end
