module FoldSyntaxTest do
  export [test_direct_call/0, test_step_by_step/0]
  
  def test_direct_call(): Int =
    let adder = make_add(5)
    adder(10, 20)  # This should fail if it's not two-argument
  
  def test_step_by_step(): Int =
    let adder = make_add(5)
    let partial = adder(10)
    partial(20)
    
  def make_add(x: Int): Int -> Int -> Int = 
    fn(y) -> fn(z) -> x + y + z end end
    
end