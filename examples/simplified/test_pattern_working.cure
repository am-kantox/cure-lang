module TestPatternWorking do
  export [main/0, test_simple/1]

  def test_simple(n: Int): Int =
    match n do
      0 -> 10
      1 -> 20
      _ -> 30
    end

  def main(): Int = 
    let result1 = test_simple(0) in
    let result2 = test_simple(1) in
    let result3 = test_simple(5) in
    result1 + result2 + result3  # Should be 10 + 20 + 30 = 60

end