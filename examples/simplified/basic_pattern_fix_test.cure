module BasicPatternFixTest do
  export [test_empty_match/0, test_head_match/0]
  
  # Test empty list pattern
  def test_empty_match(): Int =
    match [] do
      [] -> 42
      _ -> 999
    end
    
  # Test head-tail pattern - this should now work!
  def test_head_match(): Int =
    match [7] do
      [] -> 999
      [x | _] -> x
    end
    
end