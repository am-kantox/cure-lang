module MultiPatternTest do
  export [test_two_elements/0, test_three_elements/0]
  
  # Test two element list [1, 2] - should extract first element (1)
  def test_two_elements(): Int =
    match [1, 2] do
      [] -> 999
      [x | _] -> x
    end
    
  # Test three element list [5, 10, 15] - should extract first element (5) 
  def test_three_elements(): Int =
    match [5, 10, 15] do
      [] -> 999
      [h | t] -> h
    end
    
end