module TestNestedVectors do
  export [test_empty_nested/0, test_simple_nested/0, test_complex_nested/0, test_mixed_nested/0]
  
  import Std.Vector [length/1, is_empty/1]
  
  def test_empty_nested(): Int =
    let empty_nested = []
    length(empty_nested)
    
  def test_simple_nested(): Int =
    let simple_nested = [[1, 2], [3, 4]]
    length(simple_nested)
    
  def test_complex_nested(): Int =
    let complex_nested = [[1], [2, 3, 4], [5, 6]]
    length(complex_nested)
    
  def test_mixed_nested(): Int =
    let mixed = [[], [1], [2, 3, 4, 5]]
    length(mixed)
    
end