module SimpleVectorTest do
  export [test_is_empty/0]
  
  import Std.Vector [is_empty/1]
  
  def test_is_empty(): Bool =
    let empty = []
    let numbers = [1, 2, 3]
    
    let empty_result = is_empty(empty)     # Should be true
    let numbers_result = is_empty(numbers) # Should be false
    
    # Simple test: empty should be true and numbers should be false
    if empty_result
      then not numbers_result
      else false
    end
    
end