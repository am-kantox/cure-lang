module SimpleListTest do
  export [test_length/0, test_head/0, test_fold/0]
  
  import Std.List [length/1, head/2, fold/3]
  
  def test_length(): Int =
    let numbers = [1, 2, 3, 4, 5]
    length(numbers)
    
  def test_head(): Int =
    let numbers = [10, 20, 30]
    head(numbers, 0)
    
  def test_fold(): Int =
    let numbers = [1, 2, 3]
    let add = fn(x) -> fn(acc) -> x + acc end end
    fold(numbers, 0, add)

end