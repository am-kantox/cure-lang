module DebugFold do
  export [test_simple/0, test_fold/0]
  
  import Std.List [fold/3]
  
  # Test a simple operation first
  def test_simple(): Int = 
    1 + 2
  
  # Test fold with a simple function
  def test_fold(): Int =
    let numbers = [1, 2, 3]
    fold(numbers, 0, simple_add)
  
  def simple_add(x: Int): Int -> Int = 
    fn(acc) -> x + acc end

end